{
    "name": "phphleb/hlogin",
    "description": "Registration module for the HLEB2 project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Foma Tuturov",
            "email": "fomiash@yandex.ru"
        }
    ],
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Phphleb\\Hlogin\\": "/"
        },
        "exclude-from-classmap": [
            "/Optional",
            "/hleb-project"
        ]
    },
    "require": {
        "php": ">=8.2.0",
        "ext-readline": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "phphleb/framework": ">=2.0.0",
        "phphleb/debugpan": ">=2.0.0",
        "phphleb/updater": "*",
        "phphleb/adminpan": "*",
        "phphleb/muller": "*",
        "phphleb/nicejson": "*",
        "phphleb/ucaptcha": "*",
        "phphleb/spreader": "*"
    }
}
